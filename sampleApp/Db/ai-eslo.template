# ai record template for register inputs
record(ai, "$(P)$(R)") {
    field(DTYP,"asynInt32")
    field(INP,"@asynMask($(PORT) $(OFFSET) $(BITS))MODBUS_DATA")
    field(LINR,"SLOPE")
    field(ESLO,"$(ESLO)")
    field(EOFF,"$(EOFF=0)")
    field(HOPR,"$(HOPR)")
    field(LOPR,"$(LOPR)")
    field(PREC,"$(PREC)")
    field(SCAN,"$(SCAN)")
    field(EGU, "$(UNITS="")")
}

