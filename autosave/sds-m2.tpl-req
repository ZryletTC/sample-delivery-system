$(LOC):$(SYS):SEL1:VLV:02:REQ_POS.VAL

$(LOC):$(SYS):SEL1:SYNC:REQ_POS.RVAL
$(LOC):$(SYS):SEL1:SYNC:REQ_POS.VAL

$(LOC):$(SYS):SEL1:VLV_LOCK.VAL

